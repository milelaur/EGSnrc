	:start geometry:
        name = detect1
        library = egs_ndgeometry
        type = EGS_XYZGeometry
		x-planes = -20 -2.6 23.9 30
        y-planes = -20 -9.9 9.9 20
        z-planes = 100 100.1
        :start media input:
            media = water
        :stop media input:
    :stop geometry:

	### Testing blades geometry
	:start geometry:
        name = blades_test
        library = egs_gunion
        geometries = prim_coll_and_blades detect1 air air_prim_to_detector1
    :stop geometry:

