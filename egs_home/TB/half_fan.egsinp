    :start geometry:
        name = air_prim_to_detector1
        library = egs_ndgeometry
        type = EGS_XYZGeometry
        x-planes = -20 30
        y-planes = -20 20
        z-planes = 5.85 100
        :start media input:
            media = air
        :stop media input:
    :stop geometry:

